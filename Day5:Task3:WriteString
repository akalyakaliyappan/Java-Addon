package program;
import java.io.FileWriter;
import java.io.IOException;

public class WriteToFile {
	public static void main(String[] args) {
		try {
			FileWriter myWriter = new FileWriter("C://Users//TEMP.KGISLEDU//Documents//akalya.txt");
			myWriter.write("Oracle Java is the #1 programming language and development platform.");
			myWriter.close();
			System.out.println("Successfully wrote to the file.");
			}
		catch(IOException e) {
			System.out.println("An errer occurred.");
			e.printStackTrace();
		}
		

	}

}
